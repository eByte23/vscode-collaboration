{
  "name": "vscode-collaboration",
  "displayName": "vscode-collaboration",
  "description": "Real time team collaboration",
  "repository": {
    "type": "git",
    "url": "https://github.com/eByte23/vscode-collaboration.git"
  },
  "version": "0.0.1",
  "publisher": "ebyte23",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "vscode-collaboration.start",
        "title": "Start Collaborating!"
      },
      {
        "command": "vscode-collaboration.connect",
        "title": "Connect to a vscode collaboration session!"
      },
      {
        "command": "vscode-collaboration.createUser",
        "title": "Create Collaboration user"
      }
    ],
    "configurationDefaults": {
      "vscode-collaboration.userId": null,
      "vscode-collaboration.endpoint": null
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/diff-match-patch": "^1.0.31",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.64",
    "@types/request": "0.0.40",
    "@types/socket.io-client": "^1.4.29",
    "@types/uuid": "2.0.29",
    "@types/ws": "0.0.38",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "concat-stream": "^1.6.0",
    "diff-match-patch": "^1.0.0",
    "request": "^2.80.0",
    "socket.io-client": "^1.7.3",
    "uuid": "^3.0.1",
    "ws": "^2.2.0"
  }
}
